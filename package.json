    }
  },
  "dependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "jest": "^24.0.0",
    "request-promise": "^4.2.2",
    "ts-jest": "^23.10.5"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  }
